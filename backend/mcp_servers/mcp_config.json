{
  "servers": [
    {
      "name": "postgres_toolbox",
      "enabled": true,
      "connection_type": "stdio",
      "command": "./mcp_servers/toolbox",
      "args": [
        "--prebuilt",
        "postgres",
        "--stdio"
      ],
      "env_vars": {
        "POSTGRES_HOST": "localhost",
        "POSTGRES_PORT": "5432",
        "POSTGRES_DATABASE": "loyalty_analytics",
        "POSTGRES_USER": "postgres",
        "POSTGRES_PASSWORD": "postgres"
      },
      "description": "PostgreSQL database operations via MCP Toolbox"
    },
    {
      "name": "filesystem",
      "enabled": false,
      "connection_type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/yabebal/My-Works/10academy/Tenx/backend/rops_h1_poc_analytics/backend"
      ],
      "description": "File system operations for the project directory"
    }
  ]
}